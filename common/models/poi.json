{
  "name": "Poi",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "coordinates": {
      "type": "geopoint",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "poiPosts": {
      "type": "hasMany",
      "model": "PoiPost",
      "foreignKey": "poiId"
    },      
    "reviews": {
      "type": "hasMany",
      "model": "Review",
      "foreignKey": "poiId"
    },
    "checkins": {
      "type": "hasMany",
      "model": "Checkin",
      "foreignKey": "poiId"
    },
    "personMedia": {
      "type": "hasMany",
      "model": "PersonMedia",
      "foreignKey": "poiId"
    }
  },
  "acls": [],
  "methods": {}
}
