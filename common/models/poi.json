{
  "name": "Poi",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "coordinates": {
      "type": "geopoint",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "poiPosts": {
      "type": "hasMany",
      "model": "PoiPost",
      "foreignKey": ""
    },
    "reviews": {
      "type": "hasMany",
      "model": "Review",
      "foreignKey": "poiId"
    },
    "poiGalleries": {
      "type": "hasMany",
      "model": "PoiGallery",
      "foreignKey": ""
    },
    "followers": {
      "type": "hasMany",
      "model": "Person",
      "foreignKey": "",
      "through": "PoiFollow"
    },
    "tags": {
      "type": "hasMany",
      "model": "Tag",
      "foreignKey": "",
      "through": "PoiCategory"
    },
    "merchant": {
      "type": "belongsTo",
      "model": "MerchantPoi",
      "foreignKey": "merchantId"
    }
  },
  "acls": [],
  "methods": {}
}
